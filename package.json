{
  "name": "express-skeleton",
  "version": "1.0.0",
  "description": "Express skeleton",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/main.js",
    "dev": "nodemon ./src/main.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "keywords": [],
  "author": "Ricid Kumbara",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "brcypt": "^1.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prisma": "^5.8.1",
    "supertest": "^6.3.4"
  }
}
